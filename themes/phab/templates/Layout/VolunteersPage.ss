<% include TopBanner %>


<div class="container mt-100 mb-20">
    <div class="row">
        <div class="col-sm-4">
            <div class="course-intro">
                <img src="$IntroImage.ReSample('770','700').URL" class="img-responsive mb-15" alt="">
            </div>
        </div>
        <div class="col-sm-8">
            <div class="course-full">
                <h2 class="heading">$PageHeading</h2>
                <div class="personal">
                    <div>
                        <span>Document Title</span>
                        <span>PHAB Brochure</span>
                    </div>
                    <div>
                        <span>Duration</span>
                        <span>12 Months</span>
                    </div>
                    <div>
                        <span>Year</span>
                        <span>2016</span>
                    </div>
                    <div>
                        <a href="$Document.URL" target="_blank" class="btn btn-primary">Download PDF</a>
                    </div>
                </div>
                $Content
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
</div>

<div class="bgcolor3 mt-60 mb-80 pt-80">
    <div class="container">
