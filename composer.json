{
  "name": "silverstripe/installer",
  "description": "The SilverStripe Framework Installer",
  "require": {
    "php": ">=5.3.3",
    "silverstripe/cms": "3.3.0",
    "silverstripe/framework": "3.3.*",
    "silverstripe/siteconfig": "3.3.*",
    "phpmailer/phpmailer": "^5.2"
  },
  "extra": {
    "branch-alias": {
      "3.x-dev": "3.4.x-dev"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
